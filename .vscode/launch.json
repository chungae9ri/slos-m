{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "probe_rs Executable launch",
      "cwd": "${workspaceFolder}",
      //!MODIFY (or remove)
      "speed": 100,
      //!MODIFY (or remove)
      //"probe": "VID:PID:<Serial>",
      "probe": "1366:0105:000720762184",
      "runtimeExecutable": "probe-rs",
      "runtimeArgs": ["dap-server"],
      //!MODIFY
      //"chip": "STM32H745ZITx",
      //"chip": "LPC55S69JEV98",
        "chip": "LPC55S69JBD100",
        //"protocol": "swd",
        //LPC55S69JBD64
        //LPC55S69JEV98
      "flashingConfig": {
        "flashingEnabled": true,
        "haltAfterReset": true,
        "formatOptions": {
          //!MODIFY (or remove). Valid values are: 'bin', 'hex', 'elf'(default), 'idf'
          "binaryFormat": "elf"
        }
      },
      "coreConfigs": [
        {
          "coreIndex": 0,
          //!MODIFY
          //"programBinary": "Relative or fully qualified path to your programBinary",
          "programBinary": "${workspaceFolder}/target/thumbv8m.main-none-eabihf/debug/lpc55s69",
          //!MODIFY
          //"svdFile": "Relative or fully qualified path to the CMSIS-SVD file for your target core"
        }
      ],
      "env": {
        //!MODIFY (or remove)
        // If you set this variable, check the VSCode console log window for the location of the log file.
        //"RUST_LOG": "info"
      },
       // Info, Debug
      "consoleLogLevel": "Debug"
    },
    {
      "type": "cortex-debug",
      "request": "attach",
      "name": "Attach (probe-rs gdb)",
      "servertype": "external",
      "gdbTarget": "localhost:1337",
      "executable": "${workspaceFolder}/target/thumbv8m.main-none-eabihf/debug/lpc55s69",
      "device": "LPC55S69JBD100",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "probe-rs gdb"
    }
  ]
}
